		"@types/dotenv": "^6.1.1",
		"@types/eslint": "^8.56.0",
		"@types/gsap": "^1.20.2",
		"@typescript-eslint/eslint-plugin": "^7.0.0",
		"@typescript-eslint/parser": "^7.0.0",
		"autoprefixer": "^10.4.20",
		"bits-ui": "^0.22.0",
		"daisyui": "^4.12.10",
		"dotenv": "^16.4.7",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
